<body id="page-top">

    <!-- Navigation section -->
    <nav class="navbar navbar-expand-lg navbar-dark fixed-top" id="mainNav">
      <div class="container">
        <a class="navbar-brand js-scroll-trigger" href="/">Fiseek</a>
        <button class="navbar-toggler navbar-toggler-right" type="button" data-toggle="collapse" data-target="#navbarResponsive" aria-controls="navbarResponsive" aria-expanded="false" aria-label="Toggle navigation">
          Menu
          <i class="fas fa-bars"></i>
        </button>
        <div class="collapse navbar-collapse" id="navbarResponsive">
          <ul class="navbar-nav text-uppercase ml-auto">
            <li class="nav-item">
              <a class="nav-link js-scroll-trigger" href="/teachers-schedule">Maestros</a>
            </li>
            <li class="nav-item">
              <a class="nav-link js-scroll-trigger" href="/classrooms-schedule">Salones</a>
            </li>
            <li class="nav-item">
              <a class="nav-link js-scroll-trigger" href="/subjects-schedule">Materias</a>
            </li>
            <li class="nav-item">
              <a class="nav-link js-scroll-trigger" href="/advises">Asesorias</a>
            </li>
          </ul>
        </div>
      </div>
    </nav>
  
    <p></p>
    <!-- Title and Subtitle section -->
    <section class="page-section" id="services">
      <div class="container">
        <div class="row align-items-center">
          <div class="col-lg-12 text-center">
            <h3 class="section-heading text-uppercase">Maestros</h3>
            <h4 class="section-subheading text-muted">Identifica el horario del maestro que desees</h4>
          </div>
        </div>
  
          <!-- Search bar section -->
        <div class="container">
          <div class="row align-items-center">
            <form class="col justify-content-center" action="/teachers-schedule" method="GET">
              <input style="text-align:center" class="form-control mb-2" type="text" name="search" placeholder="Buscar Maestro" aria-label="Search" value="<%= searchOptions.search %>">
              <button class="btn btn-primary btn text-uppercase js-scroll-trigger" type="submit">Buscar</button>
            </form>
          </div>
        </div>
  
        <p></p>
  
        <% var currentTime = new Date()%>
        <% var currentTimeToUse = (currentTime.getHours()-5) + ":" + currentTime.getMinutes() + ":" + currentTime.getSeconds()%>
        <h3 td align="center">Clase Actual - Hora Actual:
          <% if(currentTimeToUse >= "7:00:00" && currentTimeToUse <= "7:49:59") {%>
            <span>M1</span>
          <% } else if(currentTimeToUse >= "7:50:00" && currentTimeToUse <= "8:39:59") {%>
            <span>M2</span>
          <% } else if(currentTimeToUse >= "8:40:00" && currentTimeToUse <= "9:29:59") {%>
            <span>M3</span>
          <% } else if(currentTimeToUse >= "9:30:00" && currentTimeToUse <= "10:19:59") {%>
            <span>M4</span>
          <% } else if(currentTimeToUse >= "10:20:00" && currentTimeToUse <= "11:09:59") {%>
            <span>M5</span>
          <% } else if(currentTimeToUse >= "11:10:00" && currentTimeToUse <= "11:59:59") {%>
            <span>M6</span>
          <% } else if(currentTimeToUse >= "12:00:00" && currentTimeToUse <= "12:49:59") {%>
            <span>V1</span>
          <% } else if(currentTimeToUse >= "12:50:00" && currentTimeToUse <= "13:39:59") {%>
            <span>V2</span>
          <% } else if(currentTimeToUse >= "13:40:00" && currentTimeToUse <= "14:29:59") {%>
            <span>V3</span>
          <% } else if(currentTimeToUse >= "14:30:00" && currentTimeToUse <= "15:19:59") {%>
            <span>V4</span>
          <% } else if(currentTimeToUse >= "15:20:00" && currentTimeToUse <= "16:09:59") {%>
            <span>V5</span>
          <% } else if(currentTimeToUse >= "16:10:00" && currentTimeToUse <= "16:59:59") {%>
            <span>V6</span>
          <% } else if(currentTimeToUse >= "17:00:00" && currentTimeToUse <= "17:44:59") {%>
            <span>N1</span>
          <% } else if(currentTimeToUse >= "17:45:00" && currentTimeToUse <= "18:29:59") {%>
            <span>N2</span>
          <% } else if(currentTimeToUse >= "18:30:00" && currentTimeToUse <= "19:14:59") {%>
            <span>N3</span>
          <% } else if(currentTimeToUse >= "19:15:00" && currentTimeToUse <= "19:59:59") {%>
            <span>N4</span>
          <% } else if(currentTimeToUse >= "20:00:00" && currentTimeToUse <= "20:44:59") {%>
            <span>N5</span>
          <% } else if(currentTimeToUse >= "20:45:00" && currentTimeToUse <= "21:29:59") {%>
            <span>N6</span>
          <% } else {%>
            <span>UwU</span>
          <%}%>
        </h3>
        <p></p>
  
        <!-- Data section -->
        <div class="mt-5 container">
          <div class="row justify-content-center">
            <div style="overflow-x: auto;" class="col-md-12">
            <table class="table table-bordered table-hover">
              <thead>
                <tr class="text-center">
                  <th>Hora</th>
                  <th>Dia</th>
                  <th>Salon</th>
                  <th>Maestro</th>
                  <th>Materia</th>
                </tr>
              </thead>
              <tbody>
                <% clases.forEach(clase => { %>
                <tr td align="center">
                  <td> <%= clase.period.hour %> </td>
                  <td> 
                    <% if(clase.day == 1) { %>
                      L-M-V
                    <% } %>
                    <% if(clase.day == 2) { %>
                      Martes
                    <% } %>
                    <% if(clase.day == 3) { %>
                      Jueves
                    <% } %>
                    <% if(clase.day == 4) { %>
                      SÃ¡bado
                    <% } %>  
                  </td>
                  <td> <%= clase.classroom.num %> </td>
                  <td> <%= clase.teacher.name %> </td>
                  <td> <%= clase.subject.name %> </td>
                </tr>
                <% }); %>
              </tbody>
            </table>
            </div>
          </div>
        </div>   
      </div>     
    </section>
  
    <!-- Footer -->
    <footer class="footer">
      <div class="container">
        <div class="row align-items-center">
          <div class="col-md-4">
            <span class="copyright">Copyright &copy; Fiseek 2020</span>
          </div>
      
          <div class="col-md-4">
            <ul class="list-inline quicklinks">
              <li class="list-inline-item">
                <a href="#">Privacy Policy</a>
              </li>
              <li class="list-inline-item">
                <a href="#">Terms of Use</a>
              </li>
            </ul>
          </div>
        </div>
      </div>
    </footer>
  
    
    <!-- Bootstrap core JavaScript -->
    <script src="vendor/jquery/jquery.min.js"></script>
    <script src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  
    <!-- Plugin JavaScript -->
    <script src="vendor/jquery-easing/jquery.easing.min.js"></script>
  
    <!-- Custom scripts for this template -->
    <script src="js/agency.min.js"></script>
  
  </body>